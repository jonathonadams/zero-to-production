{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "web-todos": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/web/todos",
      "sourceRoot": "apps/web/todos/src",
      "prefix": "todo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/web/todos",
            "index": "apps/web/todos/src/index.html",
            "main": "apps/web/todos/src/main.ts",
            "polyfills": "apps/web/todos/src/polyfills.ts",
            "tsConfig": "apps/web/todos/tsconfig.app.json",
            "assets": [
              "apps/web/todos/src/favicon.ico",
              "apps/web/todos/src/assets",
              "apps/web/todos/src/manifest.webmanifest"
            ],
            "styles": ["apps/web/todos/src/styles.scss"],
            "scripts": [],
            "aot": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/web/todos/src/environments/environment.ts",
                  "with": "apps/web/todos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/web/todos/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web-todos:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-todos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-todos:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/web/todos/tsconfig.app.json",
              "apps/web/todos/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/web/todos/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web/todos/jest.config.js",
            "tsConfig": "apps/web/todos/tsconfig.spec.json",
            "setupFile": "apps/web/todos/src/test-setup.ts"
          }
        }
      }
    },
    "web-todos-e2e": {
      "root": "apps/web/todos-e2e",
